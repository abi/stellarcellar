{
  "private": true,
  "name": "stellar-cellar",
  "version": "0.1.0",
  "description": "A simple, secure, cheap file syncing/sharing alternative",
  "repository": {
    "type": "git",
    "url": "https://github.com/abi/stellarcellar"
  },
  "dependencies": {
    "bcrypt": "~0.7.6",
    "connect-flash": "~0.1.1",
    "connect-mongo": "0.3.x",
    "debug": "~0.7.2",
    "express": "3.x",
    "express-validator": "~0.7.0",
    "jade": "0.32.x",
    "marked": "0.2.x",
    "mongoose": "~3.8.8",
    "passport": "0.1.x",
    "passport-local": "0.1.x",
    "passport-local-mongoose": "~0.3.0",
    "stylus": "^0.44.0",
    "watchify": "^0.8.2"
  },
  "devDependencies": {
    "nodemon": "*"
  },
  "keywords": [],
  "author": "Abi Raja",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PeerCDN/node-simple-app/issues"
  },
  "scripts": {
    "watch": "npm run watch-server & npm run watch-client",
    "watch-server": "DEBUG='*,-connect:*,-express:*,-send' nodemon .",
    "watch-client": "watchify static/js/index.js -o static/build/bundle.js & stylus -w static/css/index.styl -o static/build/",
    "update": "npm prune && npm update"
  }
}
